        <toolchain name="mcux"></toolchain>
    </toolchains>
    <families>
        <family name="mcx"></family>
    </families>
    <nxp_compatible_hw>
        <hw id="FRDM-MCXN947"></hw>
    </nxp_compatible_hw>
    <nxp_ebh_hw>
        <ebh id="nxp_frdmstbc_p3115"></ebh>
        <ebh id="mikroe_accel_pressure_click"></ebh>
    </nxp_ebh_hw>
    <peripherals>
        <peripheral name="i2c"></peripheral>
        <peripheral name="uart"></peripheral>
    </peripherals>
    <categories>
        <category name="sensor"></category>
        <category name="low_power"></category>
    </categories>
</data:item>